Astronauta astronauta = new Astronauta("Gagarin",
    new ContadorAscendente());
System.out.println("Soy " + astronauta.getNombre() +
    ": " + astronauta.cuenta());

astronauta.setContador(new ContadorDescendente());
System.out.println("Soy " + astronauta.getNombre() +
    ": " + astronauta.cuenta());
